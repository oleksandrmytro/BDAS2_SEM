<Window x:Class="BDAS2_SEM.View.PatientViews.DoctorsListView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DoctorsListView" Height="450" Width="800">
    <Grid>
        <ListView ItemsSource="{Binding Doctors}">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Имя" DisplayMemberBinding="{Binding Jmeno}" />
                    <GridViewColumn Header="Фамилия" DisplayMemberBinding="{Binding Prijmeni}" />
                    <GridViewColumn Header="Телефон" DisplayMemberBinding="{Binding Telefon}" />
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>